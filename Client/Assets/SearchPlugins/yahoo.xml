<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Yahoo</ShortName>
<InputEncoding>UTF-8</InputEncoding>
<Image width="16" height="16">data:image/png;base64,</Image>
<Url type="application/x-suggestions+json" method="GET"
     template="https://search.yahoo.com/sugg/ff">
  <Param name="output" value="fxjson" />
  <Param name="appid" value="brave_yset" />
  <Param name="command" value="{searchTerms}" />
  <Param name="nresults" value="4" />
</Url>
<!-- phone search -->
<Url type="text/html" method="GET" template="https://search.yahoo.com/search" rel="searchform">
  <Param name="ei"     value="UTF-8" />
  <Param name="fr"  value="brave_yset" />
  <Param name="p"  value="{searchTerms}" />
</Url>
<!-- tablet search -->
<Url type="application/x-moz-tabletsearch" method="GET" template="https://search.yahoo.com/search">
  <Param name="ei"     value="UTF-8" />
  <Param name="fr"  value="brave_yset" />
  <Param name="p"  value="{searchTerms}" />
</Url>
<!-- phone Search activity -->
<Url type="text/html" method="GET" rel="mobile" template="https://search.yahoo.com/search">
  <Param name="ei"     value="UTF-8" />
  <Param name="fr"  value="brave_yset" />
  <Param name="p"  value="{searchTerms}" />
</Url>
<!-- Tablet Search activity -->
<Url type="application/x-moz-tabletsearch" method="GET" 
     rel="mobile" template="https://search.yahoo.com/search">
  <Param name="ei"     value="UTF-8" />
  <Param name="fr"  value="brave_yset" />
  <Param name="p"  value="{searchTerms}" />
</Url>
</SearchPlugin>
